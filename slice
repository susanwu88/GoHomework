package main

import (
	"fmt"
)

func main() {
	// COMPOSITE LITERAL

	x := []int{1, 2, 3, 4, 5, 6, 7, 8, 42}
	fmt.Println(x)

}

// a SLICE allows you to group together VALUES of the same TYPE


package main

import (
	"fmt"
)

func main() {
	
	x := []int{1, 2, 3, 4, 5, 6, 7, 8, 42}
	fmt.Println(len(x))
	fmt.Println(cap(x))
	fmt.Println(x[5])
}

// a SLICE allows you to group together VALUES of the same TYPE
// can loop over values in a slice with range clause
