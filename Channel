//channels 

//making a channel
	//c := make(chan int)

//putting values on a channel
	//c <- 42

//taking values off of a channel
	//	<-c

//buffered channels

//

package main

import (
	"fmt"
)

func main() {
	c := make(chan int)

	go func() {

		c <- 42

	}()
	fmt.Println(<-c)

}


//buffered channel
// c:= make(chan int, 1)
