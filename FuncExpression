//Func Expression is assigning a func to a variable

package main

import (
	"fmt"
)

func main() {
	fmt.Println("Hello, playground")

	f := func() {
		fmt.Println("my first func expression")

	}
	f()
}

//

package main

import (
	"fmt"
)

func main() {
	fmt.Println("Hello, playground")

	f := func() {
		fmt.Println("my first func expression")

	}
	f()

	g := func(x int) {
		fmt.Println("The year big brother started watching", x)
	}
	g(1984)

}
