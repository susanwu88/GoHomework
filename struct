//struct isa composite data type, aggregate values of different types

package main

import (
	"fmt"
)

type person struct {
	first string
	last  string
}

func main() {

	p1 := person{
		first: "Jane",
		last:  "Bond",
	}
	p2 := person{
		first: "Miss",
		last:  "Moneypenny",
	}

	fmt.Println(p1)
	fmt.Println(p2)

	fmt.Println(p1.first, p1.last)
	fmt.Println(p2.first, p2.last)
}
