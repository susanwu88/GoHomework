package main

import (
	"fmt"
)

func main() {
	switch {
	case false:

		fmt.Println("don't print")
	case (2 == 4):
		fmt.Println("don't print")
	case (3 == 3):
		fmt.Println("print")
	case (4 == 4):
		fmt.Println("also true, does it print")
	}
}


package main

import (
	"fmt"
)

func main() {
	switch "Bond" {
	case "MoneyPenny":

		fmt.Println("Miss Money")
	case "Bond":
		fmt.Println("James Bond")
	case "Q":
		fmt.Println("This is Q")

	}
}


package main

import (
	"fmt"
)

func main() {
	n := "Bond"
	switch n {
	case "MoneyPenny":

		fmt.Println("Miss Money")
	case "Bond":
		fmt.Println("James Bond")
	case "Q":
		fmt.Println("This is Q")

	}
}

//Multiple case
package main

import (
	"fmt"
)

func main() {
	n := "Bond"
	switch n {
	case "MoneyPenny", "Bond", "Doctor No":

		fmt.Println("Miss Money or Bond or Doctor No")
	case "M":
		fmt.Println("James Bond or M")
	case "Q":
		fmt.Println("This is Q")

	}
}

package main

import (
	"fmt"
)

func main() {

	switch {
	case false:
		fmt.Println("should not print")
	case true:
		fmt.Println("should print")
	}
}
